# Application
VITE_APP_NAME="AZ Team Order Tracker"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Order tracking application for AZ Team"

# Environment
VITE_APP_ENV="development" # development, staging, production
VITE_APP_DEBUG=true

# API Configuration (when backend is implemented)
# VITE_API_URL="http://localhost:3000/api"
# VITE_API_TIMEOUT=30000

# Feature flags
VITE_FEATURE_BUSINESS_CARDS=true

# Time calculation settings
VITE_TIME_HEAT_TRANSFER_VINYL_CUT=5
VITE_TIME_HEAT_TRANSFER_VINYL_WEED=3
VITE_TIME_HEAT_TRANSFER_VINYL_PRESS=2
VITE_TIME_SUBLIMATION_PRINT=1
VITE_TIME_SUBLIMATION_PRESS=2
VITE_TIME_EMBROIDERY_DIGITIZE=15
VITE_TIME_EMBROIDERY_PRODUCTION=30
